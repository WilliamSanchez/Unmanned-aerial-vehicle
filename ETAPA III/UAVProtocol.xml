<?xml version="1.0"?>

<PropertyList>
  <generic>

    <output>
    
      <line_separator>newline</line_separator>
      <var_separator>newline</var_separator>
      
       <!-- ##### GPS ##### -->
      <chunk>
	<name>latitude</name>
	<type>double</type>
	<node>/position/latitude-deg</node>
	<format>%03.6f</format>
      </chunk> 
     
      <chunk>
	<name>longitude</name>
	<type>double</type>
	<node>/position/longitude-deg</node>
	<format>%03.6f</format>
      </chunk> 
    
      <chunk>
	<name>altitude</name>
	<type>double</type>
	<node>/position/altitude-ft</node>
	<format>%05.5f</format> 
      </chunk> 
 
         <!-- ##### AIR SPEED ##### -->
     
      <chunk>
	<name>airspeed</name>
	<type>double</type>
	<node>/velocities/airspeed-kt</node>
	<format>%03.5f</format>
      </chunk> 
 
        <!-- ##### HEADING ##### -->

<!--
      <chunk> 
	<name>heading</name>
	<type>double</type>
	<node>/orientation/heading-magnetic-deg</node>
	<format>%03.5f</format>
      </chunk>
-->

      <chunk> 
	<name>heading</name>
	<type>double</type>
	<node>/orientation/heading-deg</node>
	<format>%03.5f</format>
      </chunk>

      <!-- ##### IMU ##### -->
     
      <chunk>
	<name>x-accel</name>
	<type>double</type>
	<node>/accelerations/pilot/x-accel-fps_sec</node>
	<format>%03.5f</format>
      </chunk>
      <chunk>
	<name>y-accel</name>
	<type>double</type>
	<node>/accelerations/pilot/y-accel-fps_sec</node>
	<format>%03.5f</format>
      </chunk>
      <chunk>
	<name>z-accel</name>
	<type>double</type>
	<node>/accelerations/pilot/z-accel-fps_sec</node>
	<format>%03.5f</format>
      </chunk>

      <chunk>
	<name>roll-rate</name>
	<type>double</type>
	<node>/orientation/roll-rate-degps</node>
	<format>%03.5f</format>
      </chunk> 
      <chunk>
	<name>pitch-rate</name>
	<type>double</type>
	<node>/orientation/pitch-rate-degps</node>
	<format>%03.5f</format>
      </chunk> 
      <chunk>
	<name>yaw-rate</name>
	<type>double</type>
	<node>/orientation/yaw-rate-degps</node>
	<format>%03.5f</format>
      </chunk> 
   
   </output>

 </generic>
</PropertyList>
